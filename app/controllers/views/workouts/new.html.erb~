			<script type="text/javascript">
			   rno=2,j=0;
			   
				function addCardioField()
				{
					table = document.getElementById('cardio');
					rowCount = table.rows.length-3;
					//add label row
					row = table.insertRow(rowCount);
					row.setAttribute("align","center");
					cell = row.insertCell(0);
					cell.innerHTML="Time";
					cell = row.insertCell(1);
					cell.innerHTML="Excersice";
					cell = row.insertCell(2);
					cell.innerHTML="Time";
					cell = row.insertCell(3);
					cell.innerHTML="Calories";
					//add field row
					rowCount = table.rows.length-3;
					row = table.insertRow(rowCount);
					cell = row.insertCell(0);
					element1 = document.createElement("input");		
					element1.type = "text";
					//element1.classField="sz";
					element1.size="5";
					element1.name = "";
					cell.appendChild(element1);
					selectElement = document.createElement("select");
					selectElement.name="";
					optionElement = document.createElement("option");
					optionElement.text="Am";
					optionElement.value="am";
					selectElement.appendChild(optionElement);

					optionElement = document.createElement("option");
					optionElement.text="Pm";
					optionElement.value="pm";
					selectElement.appendChild(optionElement);
					cell.appendChild(selectElement);

					cell = row.insertCell(1);
					element1 = document.createElement("input");		
					element1.type = "text";
					element1.size="20";
					element1.name = "";
					cell.appendChild(element1);

					cell = row.insertCell(2);
					element1 = document.createElement("input");		
					element1.type = "text";
					element1.size="12";
					element1.name = "";
					cell.appendChild(element1);

					cell = row.insertCell(3);
					cell.style.fontSize="24px";
					cell.innerHTML="245";
				}
				function addExerciseField(comp)
				{
				  reg=new RegExp("[0-9]{1}");
				  j=s1=document.getElementById(comp.id).value;
				  if(s1!="" && reg.test(s1) && s1<5)
				  {
					table = document.getElementById('weight');
					for(i=1;i<s1;i++)
					{
					rno++;
					rowCount = table.rows.length-3;
					//add field row
					row = table.insertRow(rowCount);
					cell = row.insertCell(0);//for empty cell
					cell = row.insertCell(1);
					element1 = document.createElement("input");
					element1.type = "text";
					element1.size="15";
					element1.setAttribute("readonly",true);
					element1.value=document.getElementById("weightExcer1").value;
					element1.name = "";
					cell.appendChild(element1);
					cell = row.insertCell(2);//for empty cell
					
					cell = row.insertCell(3);
					element1 = document.createElement("input");
					element1.type = "text";
					element1.size="1";
					element1.name = "";
					cell.appendChild(element1);
					
					cell = row.insertCell(4);
					element1 = document.createElement("input");
					element1.type = "text";
					element1.size="3";
					element1.name = "";
					cell.appendChild(element1);
					
					cell = row.insertCell(5);
					element1 = document.createElement("input");
					element1.type = "text";
					element1.size="3";
					element1.name = "";
					cell.appendChild(element1);
					
					cell = row.insertCell(6);
					element1 = document.createElement("input");
					element1.type = "text";
					element1.size="3";
					element1.name = "";
					cell.appendChild(element1);
					
					cell = row.insertCell(7);
					element1 = document.createElement("input");
					element1.type = "text";
					element1.size="5";
					element1.name = "";
					cell.appendChild(element1);
					
					cell = row.insertCell(8);
					}
				  }
				  else
				  {
				    c=0;
				    for(i=rno;i>2+j;i--)
				    {
							c++;//alert(rno);
							document.getElementById('weight').deleteRow(i);
							//alert("row "+i+" del");
				    }
				    rno-=c;
				    //alert(rno);
				  }
				}
				function addWeightField()
				{
					table = document.getElementById('weight');
					rowCount = table.rows.length-3;
					//add field row
					row = table.insertRow(rowCount);
					row.setAttribute("align","center");
					cell = row.insertCell(0);
					cell.innerHTML="Time";
					cell = row.insertCell(1);
					cell.innerHTML="Excersice";
					cell = row.insertCell(2);
					cell.innerHTML="Sets";
					cell = row.insertCell(3);
					cell.innerHTML="Reps";
					cell = row.insertCell(4);
					cell.innerHTML="Weight";
					cell = row.insertCell(5);
					cell.innerHTML="TUT";
					cell = row.insertCell(6);
					cell.innerHTML="Rest";
					cell = row.insertCell(7);
					cell.innerHTML="Time";
					cell = row.insertCell(8);
					cell.innerHTML="Calories";
					rno++;//new row create 
					
					rowCount = table.rows.length-3;
					//add field row
					row = table.insertRow(rowCount);
					cell = row.insertCell(0);
					element1 = document.createElement("input");		
					element1.type = "text";
					element1.size="5";
					element1.name = "";
					cell.appendChild(element1);
					selectElement = document.createElement("select");
					selectElement.name="";
					optionElement = document.createElement("option");
					optionElement.text="Am";
					optionElement.value="am";
					selectElement.appendChild(optionElement);

					optionElement = document.createElement("option");
					optionElement.text="Pm";
					optionElement.value="pm";
					selectElement.appendChild(optionElement);
					cell.appendChild(selectElement);
					
					cell = row.insertCell(1);
					element1 = document.createElement("input");
					element1.type = "text";
					element1.size="15";
					element1.setAttribute("readonly",true);
					element1.value=document.getElementById("weightExcer1").value;
					element1.name = "";
					cell.appendChild(element1);
					
					cell = row.insertCell(2);
					element1 = document.createElement("input");
					element1.type = "text";
					element1.size="1";
					element1.name = "";
					element1.id="setsId"+rno;
					element1.setAttribute("onkeyup","addExerciseField(this)");
					cell.appendChild(element1);
					
					cell = row.insertCell(3);
					element1 = document.createElement("input");
					element1.type = "text";
					element1.size="1";
					element1.name = "";
					cell.appendChild(element1);
					
					cell = row.insertCell(4);
					element1 = document.createElement("input");
					element1.type = "text";
					element1.size="3";
					element1.name = "";
					cell.appendChild(element1);
					
					cell = row.insertCell(5);
					element1 = document.createElement("input");
					element1.type = "text";
					element1.size="3";
					element1.name = "";
					cell.appendChild(element1);
					
					cell = row.insertCell(6);
					element1 = document.createElement("input");
					element1.type = "text";
					element1.size="3";
					element1.name = "";
					cell.appendChild(element1);
					
					cell = row.insertCell(7);
					element1 = document.createElement("input");
					element1.type = "text";
					element1.size="5";
					element1.name = "";
					cell.appendChild(element1);
					
					cell = row.insertCell(8);
					cell.innerHTML="220";
					rno++;
				}
			</script>

