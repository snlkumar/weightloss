<script type="text/javascript">

 $(document).ready(function(){
    $("select#vendor_year").change(function(){
        d1=new Date($(this).val());
        d2=new Date();
        $("#cal_year").val(d2.getFullYear()-d1.getFullYear());
    });
 });


<% content_for :js do %>
    $('#distance-slider').slider({
          range: "min",
          step:  10,
          value: 0,
          min: 0,
          max: 500,
          slide: function( event, ui ) {
            miles=ui.value;
            $( "#distance-view" ).html(miles);
            $( "#distance-val" ).val( ui.value );
          }
        });
    
    miles= $( "#distance-slider" ).slider( "value" );
    
    $( "#distance-view" ).html( miles);
  <% end %>

</script>

