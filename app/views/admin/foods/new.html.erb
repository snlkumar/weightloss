<div class="grid_12">
  <%= link_to 'Back', admin_foods_path %>
  
  <h3>New food</h3>
  
  <% form_for(@food) do |f| %>
    <%= f.error_messages %>
    
    <%= render :partial => 'form', :locals => {:f => f} %>
    
    <p>
      <%= f.submit 'Create' %>
    </p>
  <% end %>
  
  <%= link_to 'Back', admin_foods_path %>
  
</div>