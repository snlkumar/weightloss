<h1>New user</h1>

<% form_for([:admin, @user], :html => {:multipart => true}) do |f| %>
  <%= f.error_messages %>
  
  <%= render :partial => 'form', :locals => {:f => f} %>
  
  <%= render :partial => '/users/personal_info', :locals => {:f => f} %>
  
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', admin_users_path %>

